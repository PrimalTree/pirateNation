# --- Supabase (Service Role key is required for writes; keep server-side only) ---
SUPABASE_URL=https://YOUR-PROJECT.supabase.co
SUPABASE_SERVICE_ROLE=YOUR_SERVICE_ROLE_KEY

# --- Mock scores source (served by Next.js dev on port 3000) ---
MOCK_SCORES_URL=http://localhost:3000/mock/scores.json

# --- Polling interval (ms) ---
LIVE_POLL_INTERVAL_MS=30000

# Optional compatibility: some places use this env name
# SUPABASE_SERVICE_ROLE_KEY=

